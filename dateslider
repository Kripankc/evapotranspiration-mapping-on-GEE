// Use a DateSlider to create annual composites of this collection.

// Use the start of the collection and now to bound the slider.
var start = ee.Image(modis.first()).date().get('year').format();
var now = Date.now();
var end = ee.Date(now).format();

// Run this function on a change of the dateSlider.
var showMosaic = function(range) {
  var et =  ee.ImageCollection('MODIS/006/MOD16A2')
            .filterDate(start,end)
            .select('ET')
  // Asynchronously compute the name of the composite.  Display it.
  range.start().get('year').evaluate(function(name) {
   
    var layer = ui.Map.Layer(et, EvaVis, name + ' composite');
    Map.layers().set(0, layer);
  });
};

// Asynchronously compute the date range and show the slider.
var dateRange = ee.DateRange(start, end).evaluate(function(range) {
  var dateSlider = ui.DateSlider({
    start: range['dates'][0],
    end: range['dates'][1],
    value: null,
    period: 20,
    onChange: showMosaic
  });
  Map.add(dateSlider.setValue(now));
});
